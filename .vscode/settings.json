{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "node_modules/": true,
        "reports/": true,
        ".vscode/": true,
        "typings/": true,
        "__pycache__": true,
        ".idea": true
